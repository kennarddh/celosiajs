		"build:watch": "concurrently \"tsc -w\" \"tsx ../../scripts/buildWatch.ts\"",
		"clean": "rimraf ./dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kennarddh/celosiajs.git"
	},
	"bugs": {
		"url": "https://github.com/kennarddh/celosiajs/issues"
	},
	"homepage": "https://github.com/kennarddh/celosiajs#readme",
	"keywords": [
		"celosiajs",
		"framework",
		"web",
		"http",
		"rest",
		"restful",
		"router",
		"api",
		"express",
		"zod"
	],
	"author": "kennarddh",
	"license": "MIT",
	"dependencies": {
		"@celosiajs/core": "^0.0.1-alpha.21"
	},
